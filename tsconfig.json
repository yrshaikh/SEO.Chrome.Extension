{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react",
    "target": "es6",
    "outDir": "dist/tsc_output",
    "sourceMap": true,
    "strict": true,
    "noEmitOnError": true
  },
  "include": [
    "src",
    "webpack.config.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "lib": ["es6"], 
  "skipLibCheck": true
}
